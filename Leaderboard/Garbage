import java.util.ArrayList;
import controlP5.*;
import processing.core.PApplet;

public class Leaderboard extends PApplet{
	
	ArrayList<Integer> rank;
	ArrayList<String> name;
	ArrayList<Integer> score;
	ControlP5 cp5;
	boolean t = false;
	
	public void settings() {
		  size(800, 800);
		}
	
	public void setup() {
		size(800,800);
		cp5 = new ControlP5(this);
		cp5.addButton("boo").setValue(0).setPosition(400,400).setSize(400,200);
		cp5.addListBox("hello");
	}
	
	public void draw() {
		background(0);
		if(t) {
			text("boo",200,100);
		}
	}
	
	public void boo() {
		println("booo");
		if(!t) {
			t = true;
		}else {
			t = false;
		}
			
	}
	
	
	public static void main(String[] args) {
		PApplet.main("Leaderboard");
	}
	
	public void populate(ArrayList<Integer> rank, ArrayList<String> name, ArrayList<Integer> score) {
		this.rank = rank;
		this.name = name;
		this.score = score;
	}
}